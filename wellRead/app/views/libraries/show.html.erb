<h1>My Library</h1>

<div class="book_pile">
    <%@my_books.each do |book| %>
    <div class="horizontal_book">
        <% edition = Edition.find(book.edition_id) %>
        <%= edition.title %>
    </div>
    <%end%>
</div>

 <a href="<%= user_path %>" class="my_library"><button>My Profile</button></a>

  <a href="<%= books_path %>" class="my_library"><button>Book Search</button></a>
